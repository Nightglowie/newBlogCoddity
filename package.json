{
  "name": "kalinchernev.github.io",
  "version": "2.0.0",
  "description": "My blog with Gatsby.js",
  "private": true,
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf public .cache",
    "deploy": "gh-pages -d public -b master",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "generate-app-icons": "sh ./scripts/generate-app-icons.sh",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint": "run-p lint:*",
    "precommit": "lint-staged",
    "release": "run-s clean build deploy",
    "start": "yarn develop",
    "test:lint": "npm run lint",
    "test": "run-p test:* -cn"
  },
  "keywords": ["gatsby"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalinchernev/kalinchernev.github.io.git"
  },
  "author":
    "Kalin Chernev <kalin.chernev@gmail.com> (https://kalinchernev.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kalinchernev/kalinchernev.github.io/issues"
  },
  "homepage": "https://kalinchernev.github.io",
  "dependencies": {
    "dateformat": "^3.0.2",
    "gatsby": "^1.9.261",
    "gatsby-link": "^1.6.44",
    "gatsby-plugin-catch-links": "^1.0.22",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-manifest": "^1.0.22",
    "gatsby-plugin-nprogress": "^1.0.7",
    "gatsby-plugin-offline": "^1.0.16",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-sharp": "^1.6.44",
    "gatsby-plugin-typography": "^1.7.10",
    "gatsby-remark-autolink-headers": "^1.4.18",
    "gatsby-remark-copy-linked-files": "^1.5.32",
    "gatsby-remark-images": "^1.5.63",
    "gatsby-remark-prismjs": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^1.4.13",
    "gatsby-source-filesystem": "^1.5.36",
    "gatsby-transformer-remark": "^1.7.41",
    "prismjs": "^1.14.0",
    "prop-types": "^15.6.1",
    "react-helmet": "^5.2.0",
    "typography-theme-moraga": "^0.15.10"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "gatsby-cli": "^1.1.52",
    "gatsby-plugin-eslint": "^1.0.3",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "sharp-cli": "^1.7.0"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  }
}
